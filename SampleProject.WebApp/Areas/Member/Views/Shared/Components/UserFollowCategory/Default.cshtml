
@model List<Category>


<div class="text-center">
    <h5>Takip ettiğiniz kategoriler</h5>

    @if (Model.Count>0)
    {
        @foreach (var item in Model)
        {
            <a  asp-action="Filter" asp-controller=""  asp-route-id="@item.ID"  >@item.Name</a><br />
        }
    }
    else
    {
            <h6>Şuan takip ettiğiniz kategori yoktur..</h6>
            <a asp-action="List"  asp-controller="Category" >Bakmak için tıklayınız...</a>
    }



</div>
